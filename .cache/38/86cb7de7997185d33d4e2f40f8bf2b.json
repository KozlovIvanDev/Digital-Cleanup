w;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  color: black;\r\n}\r\n\r\n.profile_img {\r\n  width: 70px;\r\n  height: 70px;\r\n  border-radius: 100px;\r\n  border: 3px solid var(--color-container);\r\n}\r\n#article .author_info {\r\n  display: flex;\r\n  flex-direction: row;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n#article .author_name_info {\r\n  height: 100px;\r\n  display: flex;\r\n  flex-direction: column;\r\n  text-align: left;\r\n  justify-content: center;\r\n  margin-left: 10px;\r\n}\r\n.no_padd {\r\n  padding: 0;\r\n  margin: 0;\r\n  margin-top: 5px;\r\n}\r\n.author_bio {\r\n  text-align: left;\r\n  font-size: 14px;\r\n  font-weight: 600;\r\n  margin-bottom: 20px;\r\n}\r\n.author_btn {\r\n  width: 100%;\r\n  text-align: center;\r\n  padding: 15px 20px;\r\n  font-weight: 700;\r\n  border-radius: 5px;\r\n  outline: none;\r\n  border: none;\r\n  background: var(--font-color);\r\n  cursor: pointer;\r\n}\r\n.l_p {\r\n  margin-right: 0;\r\n}\r\n.article_nav_action {\r\n  display: flex;\r\n  flex-direction: row;\r\n  align-items: center;\r\n  justify-content: space-evenly;\r\n  background: var(--component_sec);\r\n  border-radius: 5px;\r\n  margin-top: 20px;\r\n  padding: 10px;\r\n}\r\n.article_nav_action h5 {\r\n  color: var(--font-sec);\r\n  font-weight: 700;\r\n  margin-top: 10px;\r\n}\r\n.about_heading {\r\n  font-weight: 800;\r\n  color: var(--font-color);\r\n}\r\n.article_section {\r\n  margin: 35px 0;\r\n}\r\n.article_txt {\r\n  font-weight: 500;\r\n  color: var(--font-color);\r\n  line-height: 125%;\r\n}\r\n.heading_secondary {\r\n  color: var(--font-heading-pri);\r\n  margin: 5px 0;\r\n  margin-top: 15px;\r\n}\r\n.heading_primary {\r\n  color: var(--font-heading-pri);\r\n  margin-bottom: 10px;\r\n}\r\n.inside_article_tags {\r\n  margin-bottom: 20px;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"7e9ffb7cdbd9ecf6e04a380df97eee93","cacheData":{"env":{}}}